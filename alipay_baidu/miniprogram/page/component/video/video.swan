<onekit-view  Class="video-page">
<onekit-view  Class="video-contain">
  <onekit-video  src="{{src}}" Class="video" id="video" poster="https://zos.alipayobjects.com/rmsportal/SSWkSLDrrgsLFDfsjzTZ.png" autoplay="{{autoplay}}" object-fit="{{objectFit}}" loop="{{loop}}" show-fullscreen-btn="{{showFullscreenBtn}}" show-center-play-btn="{{showCenterPlayBtn}}" bindplay="onPlay" bindpause="onPause" bindended="onEnded" bindtimeUpdate="onTimeUpdate" bindloading="onLoading" bindstop="onStop" enableNative="{{true}}" muted="true">
    </onekit-video>
    </onekit-view>
    <onekit-view  Class="operation-item" style="background-color:lightblue">{{title}}</onekit-view>
    <onekit-view  Class="operation-item" bindtap="play">播放</onekit-view>
    <onekit-view  Class="operation-item" bindtap="pause">暂停</onekit-view>
    <onekit-view  Class="operation-item" bindtap="seek">跳到15s播放</onekit-view>
    <onekit-view  Class="operation-item" bindtap="mute">静音</onekit-view>
    <onekit-view  Class="operation-item" bindtap="playbackRate">倍速播放</onekit-view>
    <onekit-view  Class="operation-item" bindtap="requestFullScreen">全屏</onekit-view>
    <onekit-view  Class="operation-item" bindtap="exitFullScreen">退出全屏</onekit-view>
    <onekit-view  Class="operation-item" bindtap="showStatusBar">显示状态栏</onekit-view>
    <onekit-view  Class="operation-item" bindtap="hideStatusBar">隐藏状态栏</onekit-view>
    <onekit-view  Class="operation-item" bindtap="stop">停止播放</onekit-view>
</onekit-view>
